Module OperatorsExample

    Sub Main()
        'sum operator 
        Console.Write("5 + 3 = ") 'print a string
        Console.WriteLine(5 + 3) 'print the sum
        Console.WriteLine()

        'difference Operator
        Console.Write("5 - 3 = ") 'print a string
        Console.WriteLine(5 - 3) 'print the sum
        Console.WriteLine()

        'product opperator
        Console.Write("5 * 3 = ") 'print a string
        Console.WriteLine(5 * 3) 'print the sum
        Console.WriteLine()

        'quotient operator
        Console.Write("5 / 3 = ") 'print a string
        Console.WriteLine(5 / 3) 'print the sum
        Console.WriteLine()
    End Sub

End Module
